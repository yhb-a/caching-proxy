<!--An SDK is a toolbox that’s customized for a certain kind of job.-->

<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <RootNamespace>caching_proxy</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

	<!--Our SDK doesn't include ASP.NET Core by default, so we need to add a reference to it - if we want to build a light weight web server-->
	<ItemGroup>
		<!--Add the ASP.NET Core toolkit to my toolbox so I can handle HTTP requests, middleware, and routing.-->
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

</Project>
